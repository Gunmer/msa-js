{
  "name": "msa-js",
  "description": "maven settings administrator CLI",
  "version": "1.2.3",
  "author": "Cristiam Sosa @Gunmer",
  "bin": {
    "msa": "./bin/run"
  },
  "bugs": "https://github.com/Gunmer/msa-js/issues",
  "dependencies": {
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.3",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^2.2.1",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "chalk": "^2.4.2",
    "inquirer": "^7.0.0",
    "ora": "^3.4.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "tslib": "^1.10.0",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.15",
    "chai": "^4.2.0",
    "globby": "^10.0.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Gunmer/msa-js",
  "keywords": [
    "oclif",
    "msa",
    "maven",
    "cli",
    "terminal"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "msa",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "message": "Update available <%= chalk.gray(config.version) %> â†’ <%= chalk.yellow(latest) %>. Run <%= chalk.green('npm i -g msa-js') %> to update."
    },
    "hooks": {
      "prerun": "./lib/hooks/prerun/connect-database",
      "finally": "./lib/hooks/finally/disconnect-database"
    }
  },
  "repository": "Gunmer/msa-js",
  "scripts": {
    "execute": "./bin/run",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate -n Migration",
    "migration:create": "ts-node node_modules/.bin/typeorm migration:create -n Migration"
  },
  "types": "lib/index.d.ts"
}
